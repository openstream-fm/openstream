{
  "name": "front",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install": "run-s install:share install:client install:server install:admin install:app",
    "install:share": "cd share && npm i",
    "install:server": "cd server && npm i",
    "install:admin": "cd admin && npm i",
    "install:app": "cd app && npm i",
    "install:client": "cd packages/client && npm i",
    "install-production": "run-s install-production:share install-production:client install-production:server install-production:app",
    "install-production:share": "cd share && npm install --omit=dev --ignore-scripts",
    "install-production:server": "cd server && npm install --omit=dev --ignore-scripts",
    "install-production:admin": "cd admin && npm install --omit=dev --ignore-scripts",
    "install-production:app": "cd app && npm install --omit=dev --ignore-scripts",
    "install-production:client": "cd packages/client && npm install --omit=dev --ignore-scripts",
    "build": "run-s build:client build:server build:admin build:app",
    "build:server": "cd server && npm run build",
    "build:admin": "cd admin && npm run build",
    "build:app": "cd app && npm run build",
    "build:client": "cd packages/client && npm run build",
    "ci": "npm ci --ignore-scripts && npm run ci:packages",
    "ci:packages": "run-s ci:share ci:client ci:server ci:admin ci:app",
    "ci:share": "cd share && npm ci",
    "ci:server": "cd server && npm ci",
    "ci:admin": "cd admin && npm ci",
    "ci:app": "cd app && npm ci",
    "ci:client": "cd packages/client && npm ci",
    "dev": "run-p dev:server dev:admin dev:app",
    "dev:server": "cd server && npm run dev",
    "dev:app": "cd app && npm run dev",
    "dev:admin": "cd admin && npm run dev",
    "dev:client": "cd packages/client && npm run dev",
    "start": "cd server && npm run start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "typescript": "^5.3.3",
    "npm-run-all": "^4.1.5"
  }
}
