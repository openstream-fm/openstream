{
  "name": "front",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "install": "run-s install:share install:server install:admin install:app",
    "install:share": "cd share && npm i",
    "install:server": "cd server && npm i",
    "install:admin": "cd admin && npm i",
    "install:app": "cd app && npm i",
    "install-production": "run-s install-production:share install-production:server install-production:app",
    "install-production:share": "cd share && npm install --omit=dev --ignore-scripts",
    "install-production:server": "cd server && npm install --omit=dev --ignore-scripts",
    "install-production:admin": "cd admin && npm install --omit=dev --ignore-scripts",
    "install-production:app": "cd app && npm install --omit=dev --ignore-scripts",
    "build": "run-s build:server build:admin build:app",
    "build:server": "cd server && npm run build",
    "build:admin": "cd admin && npm run build",
    "build:app": "cd app && npm run build",
    "ci": "npm ci --ignore-scripts && npm run ci:packages",
    "ci:packages": "run-s ci:share ci:server ci:app",
    "ci:share": "cd share && npm ci",
    "ci:server": "cd server && npm ci",
    "ci:admin": "cd admin && npm ci",
    "ci:app": "cd app && npm ci"
  },
  "keywords": [],
