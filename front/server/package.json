{
  "name": "server",
  "version": "1.0.0",
  "description": "openstream-front nodejs server",
  "type": "module",
  "scripts": {
    "prepare": "ts-patch install -s",
    "start": "node --no-warnings --experimental-specifier-resolution=node openstream-front start",
    "dev": "SVELTEKIT_APP_DEV=1 SVELTEKIT_APP_PORT=3100 nodemon --exec node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm src/cli.ts start",
    "build": "tsc",
    "test": "npm run build && ava --match='!*integration*'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "chalk": "^5.2.0",
    "commander": "^9.4.1",
    "connect-mongodb-session": "^3.1.1",
    "cookie-parser": "^1.4.6",
    "dot-prop": "^7.2.0",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "kleur": "^4.1.5",
    "node-fetch": "^2.6.7",
    "nodemon": "^2.0.22",
    "qs": "^6.11.0",
    "toml": "^3.0.0",
    "ts-patch": "^2.1.0",
    "typia": "^3.4.18"
  },
  "devDependencies": {
    "@ava/typescript": "^4.0.0",
    "@types/connect-mongodb-session": "^2.4.4",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/express-session": "^1.17.5",
    "@types/node-fetch": "^2.6.2",
    "ava": "^5.2.0",
    "ttypescript": "^1.5.15",
    "type-fest": "^3.8.0",
    "typescript": "^4.9.4"
  },
  "ava": {
    "nodeArguments": [
      "--no-warnings",
      "--experimental-specifier-resolution=node"
    ],
    "typescript": {
      "extensions": [
        "ts",
        "tsx"
      ],
      "rewritePaths": {
        "src/": "dist/"
      },
      "compile": false
    }
  }
}
