{
  "name": "server",
  "version": "1.0.0",
  "description": "openstream-front nodejs server",
  "type": "module",
  "scripts": {
    "prepare": "ts-patch install -s & typia patch",
    "start": "node dist/cli.js start",
    "dev:run": "SVELTEKIT_APP_DEV=1 SVELTEKIT_APP_PORT=3100 SVELTEKIT_ADMIN_DEV=1 SVELTEKIT_ADMIN_PORT=5100 nodemon --no-warnings --experimental-specifier-resolution=node dist/cli.js start",
    "dev:compile": "tsc -w",
    "dev": "run-p dev:compile dev:run",
    "build": "tsc",
    "test": "npm run build && ava --match='!*integration*'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@openstream/client": "^0.1.4",
    "accept-language-parser": "^1.5.0",
    "braintree": "^3.15.0",
    "commander": "^9.4.1",
    "comment-json": "^4.2.3",
    "cookie-parser": "^1.4.6",
    "dot-prop": "^7.2.0",
    "express": "^4.18.2",
    "find-up": "^6.3.0",
    "http-status-codes": "^2.2.0",
    "kleur": "^4.1.5",
    "node-fetch": "^2.6.7",
    "qs": "^6.11.0",
    "toml": "^3.0.0",
    "typia": "^5.4.1"
  },
  "devDependencies": {
    "@types/accept-language-parser": "^1.5.3",
    "@types/braintree": "^3.3.8",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/node-fetch": "^2.6.2",
    "ava": "^5.2.0",
    "nodemon": "^2.0.22",
    "openai": "^4.24.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "ts-patch": "^3.1.2",
    "ttypescript": "^1.5.15",
    "type-fest": "^3.8.0",
    "typescript": "^5.3.2"
  },
  "ava": {
    "nodeArguments": [
      "--no-warnings",
      "--experimental-specifier-resolution=node"
    ],
    "typescript": {
      "extensions": [
        "ts",
        "tsx"
      ],
      "rewritePaths": {
        "src/": "dist/"
      },
      "compile": false
    }
  }
}
